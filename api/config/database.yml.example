test:
  adapter: postgresql
  encoding: utf8
  database: venue_test
  pool: 5
  timeout: 5000
  username: venue
  password: venue

production:
  adapter: postgresql
  encoding: utf8
  database: venue
  pool: 5
  timeout: 5000
  username: venue
  password: venue

legacy_db:
  adapter: postgresql
  encoding: utf8
